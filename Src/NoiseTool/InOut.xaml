<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:local="clr-namespace:NoiseTool"
	mc:Ignorable="d"
	x:Class="NoiseTool.InOut"
	x:Name="UserControl"
	d:DesignWidth="163" d:DesignHeight="97.04">

	<Grid x:Name="LayoutRoot">
		<Rectangle x:Name="InOut_Copy"/>
		<local:Link x:Name="xOut" HorizontalAlignment="Right" Margin="0" Width="14" Height="16" ToolTip="{Binding Out}"/>
		<local:Link x:Name="xIn1" HorizontalAlignment="Left" Margin="0" Width="14" Height="16" ToolTip="{Binding In1}"/>
		<local:Link x:Name="xIn2" HorizontalAlignment="Left" Margin="0" Width="14" VerticalAlignment="Top" Height="16" ToolTip="{Binding In2}"/>
		<local:Link x:Name="xIn3" HorizontalAlignment="Left" Margin="0" Width="14" VerticalAlignment="Bottom" Height="16" ToolTip="{Binding In3}"/>
		<Grid x:Name="xGrid" HorizontalAlignment="Left" Margin="0" VerticalAlignment="Bottom" Width="14" Height="55">
			<local:Link x:Name="xIn4" ToolTip="{Binding In4}" HorizontalAlignment="Left" VerticalAlignment="Center"/>
		</Grid>
	</Grid>
</UserControl>